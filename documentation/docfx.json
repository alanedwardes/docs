{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "BinaryMapper/src/**.csproj"
          ],
          "src": "../libraries"
        }
      ],
      "dest": "BinaryMapper"
    },
    {
      "src": [
        {
          "files": [
            "Estranged.Lfs/src/**.csproj"
          ],
          "src": "../libraries"
        }
      ],
      "dest": "Estranged.Lfs"
    },
    {
      "src": [
        {
          "files": [
            "Ae.Dns/src/**.csproj"
          ],
          "src": "../libraries"
        }
      ],
      "dest": "Ae.Dns"
    },
    {
      "src": [
        {
          "files": [
            "Ae.Synthetics/src/**.csproj"
          ],
          "src": "../libraries"
        }
      ],
      "dest": "Ae.Synthetics"
    },
    {
      "src": [
        {
          "files": [
            "Ae.Freezer/src/**.csproj"
          ],
          "src": "../libraries"
        }
      ],
      "dest": "Ae.Freezer"
    },
    {
      "src": [
        {
          "files": [
            "Ae.ImGuiBootstrapper/src/**.csproj"
          ],
          "src": "../libraries"
        }
      ],
      "dest": "Ae.ImGuiBootstrapper"
    }
  ],
  "build": {
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "content": [
      {
        "files": [
          "*/*.md"
        ],
        "src": "../libraries"
      },
      {
        "files": [
          "**/**.yml",
          "**/index.md"
        ]
      },
      {
        "files": [
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "*/*.png"
        ],
        "src": "../libraries"
      },
      {
        "files": [
          "logo.png"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appLogoPath": "logo.png"
    }
  }
}
