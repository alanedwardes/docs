{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "BinaryMapper/src/**.csproj"
          ],
          "src": "../libraries"
        }
      ],
      "dest": "BinaryMapper"
    },
    {
      "src": [
        {
          "files": [
            "Estranged.Lfs/src/**.csproj"
          ],
          "src": "../libraries"
        }
      ],
      "dest": "Estranged.Lfs"
    },
    {
      "src": [
        {
          "files": [
            "Ae.Dns/src/**.csproj"
          ],
          "src": "../libraries"
        }
      ],
      "dest": "Ae.Dns"
    },
    {
      "src": [
        {
          "files": [
            "Ae.Synthetics/src/**.csproj"
          ],
          "src": "../libraries"
        }
      ],
      "dest": "Ae.Synthetics"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "*/*.md"
        ],
        "src": "../libraries"
      },
      {
        "files": [
